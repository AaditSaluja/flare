Cell[CellGroupData[{
Cell["Calculate overlap of basis functions.", "Section",
 CellChangeTimes->{{3.74638127845807*^9, 3.746381301609829*^9}, {
  3.7463837243423977`*^9, 3.746383725959597*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PhiN", " ", "=", " ", 
  RowBox[{"Exp", "[", 
   FractionBox[
    RowBox[{"-", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"r", "-", 
        FractionBox[
         RowBox[{"rcut", " ", "n"}], "nmax"]}], ")"}], "2"]}], 
    RowBox[{"2", " ", 
     SuperscriptBox["sig", "2"]}]], "]"}]}]], "Input",
 CellChangeTimes->{{3.7463800374118633`*^9, 3.746380100806102*^9}, {
  3.7463802057413387`*^9, 3.746380210406664*^9}}],

Cell[BoxData[
 SuperscriptBox["\[ExponentialE]", 
  RowBox[{"-", 
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"r", "-", 
       FractionBox[
        RowBox[{"n", " ", "rcut"}], "nmax"]}], ")"}], "2"], 
    RowBox[{"2", " ", 
     SuperscriptBox["sig", "2"]}]]}]]], "Output",
 CellChangeTimes->{3.746380211408148*^9, 3.7463813612025967`*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"PhiNPrime", " ", "=", " ", 
  RowBox[{"Exp", "[", 
   FractionBox[
    RowBox[{"-", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"r", "-", 
        FractionBox[
         RowBox[{"rcut", " ", "nprime"}], "nmax"]}], ")"}], "2"]}], 
    RowBox[{"2", " ", 
     SuperscriptBox["sig", "2"]}]], "]"}]}]], "Input",
 CellChangeTimes->{{3.7463801547765207`*^9, 3.746380165715509*^9}, {
  3.746380254503668*^9, 3.746380256772668*^9}}],

Cell[BoxData[
 SuperscriptBox["\[ExponentialE]", 
  RowBox[{"-", 
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"r", "-", 
       FractionBox[
        RowBox[{"nprime", " ", "rcut"}], "nmax"]}], ")"}], "2"], 
    RowBox[{"2", " ", 
     SuperscriptBox["sig", "2"]}]]}]]], "Output",
 CellChangeTimes->{3.7463801709464006`*^9, 3.746380257271249*^9, 
  3.746381364298869*^9}]
}, Open  ]],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"SMat", " ", "=", " ", 
  RowBox[{
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{
      SuperscriptBox["r", "2"], "*", "PhiN", " ", "*", " ", "PhiNPrime"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "rcut"}], "}"}]}], "]"}], "//", 
   "FullSimplify"}]}]], "Input",
 CellChangeTimes->{{3.746380176511175*^9, 3.746380177564035*^9}, {
  3.746380260485564*^9, 3.74638031656688*^9}, {3.7463806210154657`*^9, 
  3.7463806272147818`*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", 
   RowBox[{"8", " ", 
    SuperscriptBox["nmax", "2"]}]], 
  RowBox[{
   SuperscriptBox["\[ExponentialE]", 
    RowBox[{"-", 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["n", "2"], "+", 
         SuperscriptBox["nprime", "2"]}], ")"}], " ", 
       SuperscriptBox["rcut", "2"]}], 
      RowBox[{"2", " ", 
       SuperscriptBox["nmax", "2"], " ", 
       SuperscriptBox["sig", "2"]}]]}]], " ", "sig", " ", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"2", " ", "nmax", " ", 
      RowBox[{"(", 
       RowBox[{"n", "+", "nprime", "-", 
        RowBox[{
         SuperscriptBox["\[ExponentialE]", 
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "nmax", "+", "nprime"}], ")"}], " ", 
            SuperscriptBox["rcut", "2"]}], 
           RowBox[{"nmax", " ", 
            SuperscriptBox["sig", "2"]}]]], " ", 
         RowBox[{"(", 
          RowBox[{"n", "+", 
           RowBox[{"2", " ", "nmax"}], "+", "nprime"}], ")"}]}]}], ")"}], " ",
       "rcut", " ", "sig"}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       FractionBox[
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"n", "+", "nprime"}], ")"}], "2"], " ", 
         SuperscriptBox["rcut", "2"]}], 
        RowBox[{"4", " ", 
         SuperscriptBox["nmax", "2"], " ", 
         SuperscriptBox["sig", "2"]}]]], " ", 
      SqrtBox["\[Pi]"], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"n", "+", "nprime"}], ")"}], "2"], " ", 
         SuperscriptBox["rcut", "2"]}], "+", 
        RowBox[{"2", " ", 
         SuperscriptBox["nmax", "2"], " ", 
         SuperscriptBox["sig", "2"]}]}], ")"}], " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Erf", "[", 
         FractionBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "+", "nprime"}], ")"}], " ", "rcut"}], 
          RowBox[{"2", " ", "nmax", " ", "sig"}]], "]"}], "-", 
        RowBox[{"Erf", "[", 
         FractionBox[
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "-", 
             RowBox[{"2", " ", "nmax"}], "+", "nprime"}], ")"}], " ", 
           "rcut"}], 
          RowBox[{"2", " ", "nmax", " ", "sig"}]], "]"}]}], ")"}]}]}], 
    ")"}]}]}]], "Output",
 CellChangeTimes->{
  3.746380177769287*^9, {3.746380264051174*^9, 3.7463803182560787`*^9}, 
   3.746380371761228*^9, 3.746380627618814*^9, 3.7463813725431757`*^9}]
}, Open  ]],

